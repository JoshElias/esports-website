{
  "name": "archivedDraftCard",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "cardId": {},
    "redbullDraftId": {},
    "cardQuantity": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "author": {
      "type": "belongsTo",
      "model": "card",
      "foreignKey": "cardId"
    },
    "draft": {
      "type": "belongsTo",
      "model": "redbullDraft",
      "foreignKey": "redbullDraftId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "accessType": "READ"
    },
    {
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$redbullAdmin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
