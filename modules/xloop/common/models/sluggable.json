{
  "name": "sluggable",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "slugId": {},
    "articleId": {},
    "forumPostId": {},
    "forumThreadId": {},
    "guideId": {},
    "snapshotId": {}
  },
  "relations": {
    "slug": {
      "type": "belongsTo",
      "model": "slug",
      "foreignKey": "slugId",
      "DestroyOnDelete": true
    },
    "article": {
      "type": "belongsTo",
      "model": "article",
      "foreignKey": "articleId"
    },
    "forumPost": {
      "type": "belongsTo",
      "model": "forumPost",
      "foreignKey": "forumPostId"
    },
    "forumThread": {
      "type": "belongsTo",
      "model": "forumThread",
      "foreignKey": "forumThreadId"
    },
    "guide": {
      "type": "belongsTo",
      "model": "guide",
      "foreignKey": "guideId"
    },
    "snapshot": {
      "type": "belongsTo",
      "model": "snapshot",
      "foreignKey": "snapshotId"
    }
  },
  "mixins": {
    "DestroyOnDelete": true,
    "Scope": true
  }
}
