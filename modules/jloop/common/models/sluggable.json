{
  "name": "sluggable",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "$destroyOnDelete": true
  },
  "properties": {
    "slugId": {},
    "articleId": {},
    "forumPostId": {},
    "forumThreadId": {},
    "guideId": {},
    "snapshotId": {},
    "hotsSnapshotId": {}
  },
  "validations": [],
  "relations": {
    "slug": {
      "type": "belongsTo",
      "model": "slug",
      "foreignKey": "slugId",
      "$destroyOnDelete": true
    },
    "article": {
      "type": "belongsTo",
      "model": "article",
      "foreignKey": "articleId"
    },
    "forumPost": {
      "type": "belongsTo",
      "model": "forumPost",
      "foreignKey": "forumPostId"
    },
    "forumThread": {
      "type": "belongsTo",
      "model": "forumThread",
      "foreignKey": "forumThreadId"
    },
    "guide": {
      "type": "belongsTo",
      "model": "guide",
      "foreignKey": "guideId"
    },
    "snapshot": {
      "type": "belongsTo",
      "model": "snapshot",
      "foreignKey": "snapshotId"
    },
    "hotsSnapshot": {
      "type": "belongsTo",
      "model": "hotsSnapshot",
      "foreignKey": "hotsSnapshotId"
    }
  },
  "acls": [],
  "methods": {}
}
