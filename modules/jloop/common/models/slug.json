{
  "name": "slug",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "slug": {
      "type": "string",
      "$validate": ["unique"]
    },
    "linked": {
      "type": "boolean"
    },
    "baseKey": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "sluggables": {
      "type": "hasMany",
      "model": "sluggable",
      "foreignKey": "slugId"
    },
    "articles": {
      "type": "hasMany",
      "model": "article",
      "foreignKey": "slugId",
      "through": "sluggable"
    }
  },
  "acls": [],
  "methods": {}
}
