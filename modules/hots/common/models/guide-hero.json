{
  "name": "guideHero",
  "plural": "guideHeroes",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "guideId": {
      "index": 1
    },
    "heroId": {
      "index": 1
    }
  },
  "validations": [],
  "relations": {
    "guide": {
      "type": "belongsTo",
      "model": "guide",
      "foreignKey": "guideId"
    },
    "hero": {
      "type": "belongsTo",
      "model": "hero",
      "foreignKey": "heroId"
    },
    "talents": {
      "type": "hasMany",
      "model": "talent",
      "foreignKey": "guideId",
      "through": "guideTalent",
      "keyThrough": "talentId"
    },
    "guideTalents": {
      "type": "hasMany",
      "model": "guideTalent",
      "foreignKey": "guideHeroId",
      "DestroyOnDelete": true
    }
  },
  "mixins": {
    "DestroyOnDelete": true,
    "Scope": true
  }
}
