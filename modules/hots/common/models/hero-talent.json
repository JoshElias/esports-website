{
  "name": "heroTalent",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "tier": {
      "type": "number",
      "index": 1
    },
    "orderNum": {
      "type": "number",
      "required": true,
      "index": 1
    },
    "heroId": {
      "index": 1
    },
    "talentId": {
      "index": 1
    },
    "abilityId": {
      "index": 1
    }
  },
  "relations": {
    "hero": {
      "type": "belongsTo",
      "model": "hero",
      "foreignKey": "heroId"
    },
    "talent": {
      "type": "belongsTo",
      "model": "talent",
      "foreignKey": "talentId"
    },
    "ability": {
      "type": "belongsTo",
      "model": "ability",
      "foreignKey": "abilityId"
    }
  },
  "mixins": {
    "Scope": true
  }
}
