{
  "name": "snapshot",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "$validate": true,
    "$docFilter": {
      "acceptedRoles": ["$admin"],
      "predicate": "inactive"
    },
    "$destroyOnDelete": true
  },
  "properties": {
    "snapNum": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "slug": {
      "url": {
        "type": "string",
        "$validate": ["unique"]
      },
      "linked": {
        "type": "boolean"
      }
    },
    "content": {
      "type": "object"
    },
    "tiers": {
      "type": "object"
    },
    "createdDate": {
      "type": "date"
    },
    "photoNames": {
      "type": "object"
    },
    "isActive": {
      "type": "boolean"
    },
    "oldAuthors": {
      "type": [
        "object"
      ]
    },
    "oldComments": {
      "type": [
        "string"
      ]
    },
    "isCommentable": {
      "type": "boolean",
      "default": true
    }
  },
  "validations": [],
  "relations": {
    "deckMatchups": {
      "type": "hasMany",
      "model": "deckMatchup",
      "foreignKey": "snapshotId",
      "$destroyOnDelete": true
    },
    "authors": {
      "type": "hasMany",
      "model": "snapshotAuthor",
      "foreignKey": "snapshotId",
      "$destroyOnDelete": true
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "snapshotId",
      "$destroyOnDelete": true
    },
    "deckTiers": {
      "type": "hasMany",
      "model": "deckTier",
      "foreignKey": "snapshotId",
      "$destroyOnDelete": true
    },
    "votes": {
      "type": "hasMany",
      "model": "vote",
      "foreignKey": "snapshotId"
    },
    "slug": {
      "type": "hasMany",
      "model": "slug",
      "foreignKey": "snapshotId",
      "through": "sluggable"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__create__comments"
    },
    {
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW",
      "property": "__create__deckTiers"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__get__deckMatchups"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__create__votes"
    },
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": "__get__votes"
    },
    {
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "__edit__votes"
    }
  ],
  "methods": {}
}
