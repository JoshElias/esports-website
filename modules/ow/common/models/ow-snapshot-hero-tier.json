{
  "name": "owSnapshotHeroTier",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "summary": {
      "type": "string"
    },
    "tier": {
      "type": "number"
    },
    "previousTiers": {
      "type": ["number"],
      "default": []
    },
    "burstScore": {
      "type": "number"
    },
    "pushScore": {
      "type": "number"
    },
    "surviveScore": {
      "type": "number"
    },
    "scaleScore": {
      "type": "number"
    },
    "utilityScore": {
      "type": "number"
    },
