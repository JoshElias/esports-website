 {
  "name": "team",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "$docFilter": {
      "acceptedRoles": ["$admin"],
      "predicate": "inactive"
    },
    "$destroyOnDelete": true
  },
  "properties": {
    "__v": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    },
    "orderNum": {
      "type": "number"
    },
    "abbreviation": {
      "type": "string"
    },
    "gameId": {}
  },
  "validations": [],
  "relations": {
    "game": {
      "type": "belongsTo",
      "model": "game",
      "foreignKey": "gameId"
    },
    "teamMembers": {
      "type": "hasMany",
      "model": "teamMember",
      "foreignKey": "teamId",
      "$destroyOnDelete": true
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
