 {
  "name": "team",
   "base": "PersistedModel",
   "strict": true,
   "idInjection": true,
   "options": {
     "validateUpsert": true
   },
  "properties": {
    "__v": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    },
    "orderNum": {
      "type": "number"
    },
    "abbreviation": {
      "type": "string"
    },
    "gameId": {}
  },
  "relations": {
    "game": {
      "type": "belongsTo",
      "model": "game",
      "foreignKey": "gameId"
    },
    "teamMembers": {
      "type": "hasMany",
      "model": "teamMember",
      "foreignKey": "teamId",
      "DestroyOnDelete": true
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    }
  ],
  "mixins": {
    "DestroyOnDelete": true,
    "Scope": true,
    "Filter": {
      "acceptedRoles": ["$admin"],
      "predicate": "inactive",
      "requiredFields": ["isActive"]
    },
    "TimeStamp" : {
      "createdAt" : "createdDate",
      "updatedAt" : "updatedDate",
      "required" : true
    }
  }
}
