 {
  "name": "team",
  "base": "xModel",
  "properties": {
    "__v": {
      "type": "number"
    },
    "name": {
      "type": "string"
    },
    "isActive": {
      "type": "boolean"
    },
    "orderNum": {
      "type": "number"
    },
    "abbreviation": {
      "type": "string"
    },
    "gameId": {}
  },
  "relations": {
    "game": {
      "type": "belongsTo",
      "model": "game",
      "foreignKey": "gameId"
    },
    "teamMembers": {
      "type": "hasMany",
      "model": "teamMember",
      "foreignKey": "teamId",
      "DestroyOnDelete": true
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    }
  ],
  "mixins": {
    "DocFilter": {
      "acceptedRoles": ["$admin"],
      "predicate": "inactive"
    },
    "DestroyOnDelete": true
  }
}
