{
  "name": "banner",
  "base": "xModel",
  "properties": {
    "bannerType": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "photoName": {
      "type": "string"
    },
    "button": {
      "type": "object"
    },
    "orderNum": {
      "type": "number"
    },
    "isActive": {
      "type": "boolean"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    }
  ],
  "mixins": {
    "DocFilter": {
      "acceptedRoles": ["$admin"],
      "predicate": "inactive"
    }
  }
}
