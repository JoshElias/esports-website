{
  "name": "article",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "object"
    },
    "description": {
      "type": "string"
    },
    "content": {
      "type": "string"
    },
    "photoNames": {
      "type": "object"
    },
    "classTags": {
      "type": [
        "string"
      ]
    },
    "themeName": {
      "type": "string"
    },
    "viewCount": {
      "type": "number"
    },
    "votes": {
      "type": [
        "object"
      ]
    },
    "votesScore": {
      "type": "number"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "createdDate": {
      "type": "date"
    },
    "premium": {
      "type": "object"
    },
    "articleType": {
      "type": [
        "string"
      ]
    },
    "isActive": {
      "type": "boolean"
    },
    "__v": {
      "type": "number"
    }
  },
  "validations": [],
  "relations": {
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "articleId"
    },
    "author": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "authorId"
    },
    "relatedArticles": {
      "type": "hasMany",
      "model": "article",
      "foreignKey": "relatedArticleIds"
    },
    "deck": {
      "type": "hasOne",
      "model": "deck",
      "foreignKey": "articleIds"
    }
  },
  "acls": [],
  "methods": {}
}
