{
  "name": "teamMember",
  "base": "xModel",
  "properties": {
    "screenName": {
      "type": "string"
    },
    "fullName": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "social": {
      "type": "object"
    },
    "photoName": {
      "type": "string"
    },
    "game": {
      "type": "string"
    },
    "orderNum": {
      "type": "number"
    },
    "isActive": {
      "type": "boolean"
    },
    "teamId": {
      "type": {}
    }
  },
  "relations": {
    "team": {
      "type": "belongsTo",
      "model": "team",
      "foreignKey": "teamId"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    }
  ],
  "mixins": {
    "DocFilter": {
      "acceptedRoles": ["$admin"],
      "predicate": "inactive"
    }
  }
}
