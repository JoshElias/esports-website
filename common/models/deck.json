    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "deckId"
    },
    "matchups": {
      "type": "hasMany",
      "model": "deckMatchup",
      "foreignKey": "deckId",
      "isChild": true
    },
    "mulligans": {
      "type": "hasMany",
      "model": "mulligan",
      "foreignKey": "deckId",
      "isChild": true
    }
